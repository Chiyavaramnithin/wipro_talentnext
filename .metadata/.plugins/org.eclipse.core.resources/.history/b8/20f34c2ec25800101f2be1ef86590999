import java.util.Scanner;

public class Qestion2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String a = "welcome";
        System.out.print("Enter your name: ");
        String b = sc.nextLine();
        
        System.out.println(a + " " + b);
        
        sc.close();
    }
}
